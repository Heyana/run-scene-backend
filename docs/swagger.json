        "/api/security/whitelist/{ip}": {
            "post": {
                "description": "将IP地址添加到白名单，该IP将绕过所有安全检查",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "安全管理"
                ],
                "summary": "添加IP到白名单",
                "parameters": [
                    {
                        "type": "string",
                        "description": "IP地址（示例: 192.168.1.1）",
                        "name": "ip",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "添加成功",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "400": {
                        "description": "IP地址格式错误或已在白名单",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "500": {
                        "description": "添加失败",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            },
            "delete": {
                "description": "将IP地址从白名单中移除，该IP将重新受到安全检查",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "安全管理"
                ],
                "summary": "从白名单移除IP",
