                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            }
        },
        "/api/resources": {
            "get": {
                "description": "获取系统中所有资源文件的列表，支持分页和类型筛选",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "资源管理"
                ],
                "summary": "获取资源文件列表",
                "parameters": [
                    {
                        "type": "integer",
                        "default": 1,
                        "description": "页码，默认1",
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 10,
                        "description": "每页数量，默认10，最大100",
                        "name": "page_size",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "资源类型筛选：image | video | audio | document",
                        "name": "type",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "媒体库ID筛选",
                        "name": "library_id",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/response.Response"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/definitions/api.ResourceListResponse"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "500": {
                        "description": "查询失败",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            }
        },
        "/api/resources/upload": {
            "post": {
                "description": "上传各类资源文件（图片、文档、音频、视频等）到系统",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "资源管理"
                ],
                "summary": "上传资源文件",
                "parameters": [
                    {
                        "type": "file",
                        "description": "资源文件（最大10MB）",
                        "name": "file",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "媒体库ID，可选",
                        "name": "library_id",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "资源类型（系统自动识别）",
                        "name": "resource_type",
                        "in": "formData"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "上传成功",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/response.Response"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/definitions/api.UploadResourceResponse"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "400": {
                        "description": "文件上传失败或文件过大",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "500": {
                        "description": "保存失败",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            }
        },
        "/api/resources/{id}": {
            "get": {
                "description": "根据ID获取资源文件，返回文件内容或重定向到CDN地址",
                "produces": [
                    "application/octet-stream",
                    "image/*",
                    "text/*"
                ],
                "tags": [
                    "资源管理"
                ],
                "summary": "获取资源文件",
                "parameters": [
                    {
                        "type": "string",
                        "description": "资源ID（示例: 1）",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "资源文件内容",
                        "schema": {
                            "type": "file"
                        }
                    },
                    "302": {
                        "description": "CDN重定向",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "无效的资源ID",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "404": {
                        "description": "资源不存在或内容不可用",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            },
            "delete": {
                "description": "永久删除指定的资源文件，包括本地和CDN存储",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "资源管理"
                ],
                "summary": "删除资源文件",
                "parameters": [
                    {
                        "type": "string",
                        "description": "资源ID（示例: 1）",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "删除成功",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "400": {
                        "description": "无效的资源ID",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "404": {
                        "description": "资源不存在",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "500": {
                        "description": "删除失败",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            }
        },
        "/api/security/block/{ip}": {
            "post": {
                "description": "将指定IP地址添加到黑名单，阻止其访问系统",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "安全管理"
                ],
                "summary": "手动封禁IP地址",
                "parameters": [
                    {
                        "type": "string",
                        "description": "IP地址（示例: 192.168.1.100）",
                        "name": "ip",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "default": 3600,
                        "description": "封禁时长(秒)，默认3600",
                        "name": "duration",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "default": "\"手动封禁\"",
                        "description": "封禁原因",
                        "name": "reason",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "封禁成功",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/response.Response"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/definitions/controllers.BlockedIP"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "400": {
                        "description": "IP地址格式错误或已被封禁",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "500": {
                        "description": "封禁失败",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            }
        },
