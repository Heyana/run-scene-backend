            "post": {
                "description": "将指定IP地址从黑名单中移除，恢复其正常访问权限",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "安全管理"
                ],
                "summary": "手动解封IP地址",
                "parameters": [
                    {
                        "type": "string",
